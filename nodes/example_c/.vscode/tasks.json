{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "skyalt_gcc",
			"command": "/usr/bin/gcc",
			"args": [
				"-O0",
				"-g",
				"-Wall",
				"-march=native",
				"-fsanitize=address",
				"${workspaceFolder}/main.c",
				"-o",
				"${workspaceFolder}/main",
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}
